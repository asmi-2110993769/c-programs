int main()
{
    int n,i,odd,even;
    printf("enter value of natural no.");
    scanf("%i",&n);
    odd=0;
    even=0;
    for(i=1;i<=n;i++)
    {
    if(i%2==0)
        even=even+i;
    else
        odd=odd+i;
    }
    printf("sum of even numbers till n is:%d",even);
    printf("\nsum of odd numbers till n is:%d",odd);
}

