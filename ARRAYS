int main()
{
    int n,i,min;
    printf("enter no.of elemets");
    scanf("%d",&n);
    int ar[n];
    for(i=0;i<n;i++)
    {
        printf("enter element at ar[%d]");
        scanf("%d",&ar[i]);
    }
    min=ar[0];
    for(i=0;i<n;i++)
    {
        if(min>ar[i])
        min=ar[i];
    }
    printf("\nmin is%d",min);
}
  
