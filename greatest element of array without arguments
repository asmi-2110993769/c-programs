int main()
{
    void largest();
    largest();
}
void largest()
{
    int i,n,a[n],largest;
    printf("enter number of elements in array: ");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        printf("\n enter element:");
        scanf("%d",&a[i]);
    }
    
    largest=a[0];
    for(i=0;i<n;i++)
    {
        if(largest<a[i])
            largest=a[i];
    }
    printf("\nlargest element of given array is %d",largest);
}
