//calculate sum of digits of a 5 integer number

int main()
{
    int n,r1,r2,r3,r4,sum;
    printf("enter 5 digit number:");
    scanf("%d",&n);
    r1=n%10;
    n=n/10;
    r2=n%10;
    n=n/10;
    r3=n%10;
    n=n/10;
    r4=n%10;
    n=n/10;
    sum=r1+r2+r3+r4+n;
    printf("\nsum is:%d",sum);
}
